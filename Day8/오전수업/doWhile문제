// do 는 일단 먼저 한 다음에 그 다음 while문으로 간다. -> 후체크 반복문 


<!DOCTYPE html>
<html lang="ko">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>dowhile</title>
</head>
<body>
  <script>
		let i = 0;
		do{
			document.write(`${i} hi<br>`);
			i++;
		}while(i<3);    
  </script>
</body>
</html>